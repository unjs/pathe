{
  "name": "pathe",
  "version": "0.2.0",
  "description": "Normalized paths for Node.js",
  "repository": "unjs/pathe",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build",
    "lint": "eslint --ext .ts .",
    "prepublishOnly": "yarn build",
    "release": "yarn vitest run && standard-version && git push --follow-tags && npm publish",
    "test": "vitest"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/node": "^16.9.6",
    "c8": "^7.11.2",
    "eslint": "latest",
    "jiti": "^1.12.3",
    "siroc": "latest",
    "standard-version": "latest",
    "typescript": "^4.4.3",
    "vitest": "^0.10.2"
  }
}
